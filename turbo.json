{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["src/**", "craco.config.js", "vue.config.js"],
			"outputs": ["dist/**"]
		},
		"build:test": {
      "dependsOn": ["^build"],
			"inputs": ["src/**", "craco.config.js", "vue.config.js"],
			"outputs": ["dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"deploy": {
			"dependsOn": ["build", "test", "lint"],
			"outputs": []
		}
	}
}
